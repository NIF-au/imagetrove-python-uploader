# ImageTrove test configuration file

[Server]
User = MyTardisSuperuser
ApiKey = MyTardisSuperuser_apikey
Url = https://mytardis.com
Institution = My Institution

[Client]
tmproot = ~\imagetrove\tmp
cores = 8

[Instrument Mapping]
SIEMENS-TrioTim = 3T Magnetom Prisma
SIEMENS-mrcTrio = 3T Magnetom Prisma
NO-INSTRUMENT = NO-INSTRUMENT

[NO-INSTRUMENT]
experiment-tag = ReferringPhysicianName
dataset-tag = PatientID
facility-name = NO-INSTRUMENT
storagebox = default

[3T Magnetom Prisma]
experiment-tag = ReferringPhysicianName
dataset-tag = PatientID
facility-name = 3T Facility
storagebox = default
